{
  "name": "seiweb/yii2-image",
  "description": "Simple Image Uploader",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "yii 2",
    "images"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Evgeny Stasenko",
      "email": "webmaster@seiweb.ru",
      "homepage": "https://seiweb.ru"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "yiisoft/yii2": "~2.0.14",
    "intervention/image": "*",
    "kartik-v/yii2-widgets": "v3.4.1",
    "kartik-v/bootstrap-fileinput": "v4.5.2",
    "seiweb/yii2-sortable": "^1.0",
    "bower-asset/font-awesome": "^5.12",
    "bower-asset/noty": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "seiweb\\image\\": ""
    }
  },
  "extra": {
    "bootstrap": "seiweb\\image\\Bootstrap"
  }
}
